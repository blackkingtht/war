using System.Collections;
using System.Collections.Generic;
using UnityEngine;

/// <summary>
/// 消息结构类
/// </summary>
public class Message {

    private long time;
    private string name;
    private string content;

    public long Time
    {
        get
        {
            return time;
        }

        set
        {
            time = value;
        }
    }

    public string Name
    {
        get
        {
            return name;
        }

        set
        {
            name = value;
        }
    }

    public string Content
    {
        get
        {
            return content;
        }

        set
        {
            content = value;
        }
    }
}
